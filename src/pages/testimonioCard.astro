---
import Layout from "../layouts/Layout.astro"
import SectionCard1 from "../components/ProyectCard/SectionCard1.astro";
import SectionCard2 from "../components/ProyectCard/sectionCard2.astro";
import SectionCard3 from "../components/ProyectCard/sectionCard3.astro";
import SectionCard4 from "../components/ProyectCard/sectionCard4.astro";
import ContenedorMain from "../utils/ContenedorMain.astro";
import Formulario from "../components/ProyectCard/formularioCard.astro";
---

<Layout title="MiniProyectos" >
  
	<ContenedorMain >
         <Formulario />
       <section class="grid grid-cols-2 grid-rows-2 gap-y-6" >
        <div class="col-span-2 lg:col-span-1">
        <SectionCard1 />
        </div>
        <div class="col-span-2 lg:col-span-1">
        <SectionCard2 />
        </div>
        <div class="col-span-2">
          <SectionCard3 />
        </div>
        <div class="col-span-2">
          <SectionCard4 />
        </div>
       
       </section>
    </ContenedorMain>
</Layout>

<script src="/src/scripts/testimonyCard.js" ></script>

